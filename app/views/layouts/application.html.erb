<!DOCTYPE html>
<html>
<head>
  <title>DnDCharacterBuilder</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'application', 'http://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic', rel: 'stylesheet', type: 'text/css' %>
  <%= stylesheet_link_tag 'application', 'http://fonts.googleapis.com/css?family=Lobster+Two:400,400italic,700', rel: 'stylesheet', type: 'text/css' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/nav' %>
  <div class="container spacer"></div>
  <div class="body-wrapper container">
    <%= render 'layouts/alerts' %>
    <%= yield %>
    <%= render 'layouts/footer'  %>
  </div>
</body>
</html>
